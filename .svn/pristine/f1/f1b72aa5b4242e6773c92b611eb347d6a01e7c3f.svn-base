	//Manage Sites
	@FindBy(linkText = "Manage Site")
	public WebElement manageSite;

	//Manage Publish
	@FindBy(linkText = "Manage Publish")
	public WebElement managePublish;

	//Manage Packaging
	@FindBy(linkText = "Packaging")
	public WebElement packaging;

